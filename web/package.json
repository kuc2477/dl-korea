{
  "name": "dl-korea",
  "version": "1.0.0",
  "description": "dl-korea",
  "main": "index.js",
  "scripts": {
    "test": "better-npm-run test",
    "check": "better-npm-run check",
    "hot-server": "better-npm-run hot-server",
    "build": "better-npm-run build",
    "lint": "eslint app test *.js"
  },
  "betterScripts": {
    "hot-server": {
      "command": "node ./server.js",
      "env": {
        "HOT_SERVER_PORT": 3000
      }
    },
    "build": {
      "command": "webpack --config webpack.config.prod.js --progress --profile --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "watch": {
      "command": "karma start ./karma.conf.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "karma start ./karma.conf.js --single-run",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "check": {
      "command": "npm-check"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuc2477/dl-korea.git"
  },
  "keywords": [
    "web",
    "service"
  ],
  "author": "Ha Junsoo",
  "license": "ISC",
  "bugs": {
